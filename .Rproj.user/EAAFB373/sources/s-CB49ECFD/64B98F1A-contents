######################
#install packages
######################
#check installed libraries
p <- as.data.frame(installed.packages())
table(p$Priority, exclude = NULL)
is.element("tidyr",installed.packages())

dim(available.packages())


#install from all 3 sources
install.packages(c("tidyr","dplyr"))

devtools::install_github("garrettgman/DSR")
library(DSR)

source("https://bioconductor.org/biocLite.R")
biocLite()
biocLite(c("GenomicFeatures", "AnnotationDbi"))

remove.packages("AnnotationDbi")
#load into memory
library(tidyr)
library(dplyr)

detach(package:dplyr)

#update
packageVersion("dplyr")
old.packages()
update.packages("R6")

#options(repos = c(CRAN = "https://mran.revolutionanalytics.com/snapshot/YYYY-MM-DD"))
#documentation
library(help="tidyr")
ls(package:tidyr)
vignette(all = TRUE)
library(Amelia)
vignette("amelia")

#find out which package a function comes from
find("apropos")


